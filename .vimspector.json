{
    "configurations": {
        "run - default": {
            "adapter": "debugpy",
            "configuration": {
                "name": "Python: Current File",
                "type": "python",
                "request": "launch",
                "program": "${file}",
                "cwd": "${workspaceRoot}",
                "console": "integratedTerminal",
                "python": "/home/kadotab/ENV/bin/python",
                "stopOnEntry": true
            },
            "breakpoints": {
                "exception": {
                    "raised": "N",
                    "uncaught": "",
                    "userUnhandled": ""
                }
            }
        },
        "MultiContrast": {
            "adapter": "debugpy",
            "configuration": {
                "name": "Multicontrast dataset",
                "type": "python",
                "request": "launch",
                "cwd": "${workspaceRoot}",
                "module": "ml_recon.dataset.Brats_dataset",
                "stopOnEntry": true,
                "python": "/home/kadotab/ENV/bin/python",
                "console": "integratedTerminal"
            },
            "breakpoints": {
                "exception": {
                    "raised": "N",
                    "uncaught": "",
                    "userUnhandled": ""
                }
            }
        },
        "Run Subset": {
            "adapter": "debugpy",
            "configuration": {
                "name": "Run Subset",
                "type": "python",
                "request": "launch",
                "cwd": "${workspaceRoot}",
                "stopOnEntry": true,
                "program": "train_model.py",
                "python": "/home/kadotab/ENV/bin/python",
                "console": "integratedTerminal",
                "args": ["--use_subset"]
            },
            "breakpoints": {
                "exception": {
                    "raised": "N",
                    "uncaught": "",
                    "userUnhandled": ""
                }
            }
        }
    }
}
